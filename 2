import java.util.Scanner;

public class CreditProgram {
    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);

        int income = scanner.nextInt();
        int expenses = scanner.nextInt();
        int creditAmount = scanner.nextInt();
        int months = scanner.nextInt();

        int netIncome = income - expenses;

        if (netIncome > creditAmount * 0.5) {
            System.out.println("Кредит одобрен");
        } else {
            System.out.println("Кредит не одобрен");
        }
    }
}
